<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomVolume">
        <attr name="backgroundVolume" format="color" />
        <attr name="lineColorVolume" format="color" />
        <attr name="textColorVolume" format="color" />
        <attr name="circleSelectedVolume" format="reference" />
        <attr name="textMin" format="reference" />
        <attr name="textMax" format="reference" />
        <attr name="textName" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomMultiSelect">
        <attr name="name" format="reference" />
        <attr name="image" format="reference" />
        <attr name="textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomSelectedView">
        <attr name="nameItem" format="reference" />
        <attr name="imageItem" format="reference" />
    </declare-styleable>
</resources>